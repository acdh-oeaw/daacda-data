         <cell role="data"><name/></cell>
         <cell role="data"><rs type="org" ref="#org__282">775th Bomb Squadron</rs></cell>
        <cell role="data"><date when-iso="1944-05-10">1944-05-10</date></cell>
         <cell role="data"><rs type="concept" ref="#concept_flak-13">Flak</rs></cell>
         <cell role="data"><rs type="place" ref="#place__22"><placeName>Wiener Neustadt</placeName></rs> </cell>
