         <cell role="data"><rs type="org" ref="#org__397">773rd Bomb Squadron</rs></cell>
        <cell role="data"><date when-iso="1945-03-20">1945-03-20</date></cell>
         <cell role="data"><rs type="concept" ref="#concept_flak-13">Flak</rs></cell>
         <cell role="data"><rs type="place" ref="#place__1018"><placeName>Wien Kagran</placeName></rs> </cell>
