         <cell role="data"><rs type="org" ref="#org__239">764th Bomb Squadron</rs></cell>
        <cell role="data"><date when-iso="1944-05-10">1944-05-10</date></cell>
         <cell role="data"><rs type="concept" ref="#concept_jager-2">Jäger</rs></cell>
